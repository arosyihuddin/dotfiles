background = #050505
background-opacity = 0.72
# Override Backspace supaya kirim DEL standar
keybind = backspace=text:\x7f

# Override Enter supaya kirim CR
keybind = enter=text:\x0d

# Override arrow keys untuk kirim CSI klasik
keybind = up=csi:A
keybind = down=csi:B
keybind = right=csi:C
keybind = left=csi:D
